type Result = 
 variant {
   err: text;
   ok;
 };
type Project = 
 record {
   description: text;
   id: nat;
   imgUrl: text;
   link: text;
   name: text;
 };
service : {
  claimDonatedCycles: (text, nat) -> (Result);
  donateCyclesToProject: (text, nat) -> (Result);
  getICPBalance: () -> (int) query;
  getProjects: () -> (vec Project) query;
}
